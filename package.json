{
  "author": "wiisportsresort",
  "name": "monument-platformer-website",
  "description": "game",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "postinstall": "node server/checkDll.js || pnpm run dll",
    "_tsconf": "cross-env TS_NODE_PROJECT=\"tsconfig-cjs.json\"",
    "dll": "pnpm run _tsconf NODE_ENV=\"production\" -- webpack --config webpack.dll.ts",
    "devserver": "pnpm run _tsconf NODE_ENV=\"development\" webpack-dev-server",
    "dev": "concurrently -n \"s,b\" -c \"reset.green,reset.blue\" \"pnpm run serve:dev\" \"pnpm run build:dev\"",
    "prod": "pnpm run build:prod; pnpm run serve:prod",
    "start": "pnpm run prod",
    "build:dev": "pnpm run _tsconf NODE_ENV=\\\"development\\\" -- webpack --watch",
    "build:prod": "pnpm run _tsconf NODE_ENV=\\\"production\\\" webpack",
    "serve:dev": "nodemon",
    "serve:prod": "pnpm run _tsconf ts-node server"
  },
  "sideEffects": [
    "*.scss"
  ],
  "dependencies": {
